@prefix wfprov: <http://purl.org/wf4ever/wfprov#> .
@prefix wfdesc: <http://purl.org/wf4ever/wfdesc#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix prov: <http://www.w3.org/ns/prov#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix dcterms: <http://purl.org/dc/terms/> .

<http://purl.org/wf4ever/wfprov> a owl:Ontology ;
    rdfs:label "Workflow Provenance Ontology"@en ;
    rdfs:comment "This ontology extends PROV-O to describe workflow execution provenance."@en ;
    dc:creator "Khalid Belhajjame" ,
               "Daniel Garijo" ,
               "Graham Klyne" ,
               "Stian Soiland-Reyes" ;
    dcterms:license <http://creativecommons.org/licenses/by/4.0/> ;
    owl:imports <http://www.w3.org/ns/prov-o#> ,
                <http://purl.org/wf4ever/wfdesc> ;
    owl:versionInfo "1.0" .

# Classes

wfprov:WorkflowRun a owl:Class ;
    rdfs:label "Workflow Run"@en ;
    rdfs:comment "An execution of a workflow."@en ;
    rdfs:subClassOf prov:Activity .

wfprov:ProcessRun a owl:Class ;
    rdfs:label "Process Run"@en ;
    rdfs:comment "An execution of a process within a workflow."@en ;
    rdfs:subClassOf prov:Activity .

wfprov:WorkflowEngine a owl:Class ;
    rdfs:label "Workflow Engine"@en ;
    rdfs:comment "A software agent that executes workflows."@en ;
    rdfs:subClassOf prov:SoftwareAgent .

wfprov:Artifact a owl:Class ;
    rdfs:label "Artifact"@en ;
    rdfs:comment "A data entity that is used or generated by a workflow execution."@en ;
    rdfs:subClassOf prov:Entity .

# Properties

wfprov:wasPartOfWorkflowRun a owl:ObjectProperty ;
    rdfs:label "was part of workflow run"@en ;
    rdfs:comment "Links a process run to the workflow run it was part of."@en ;
    rdfs:domain wfprov:ProcessRun ;
    rdfs:range wfprov:WorkflowRun ;
    rdfs:subPropertyOf prov:wasInformedBy .

wfprov:usedInput a owl:ObjectProperty ;
    rdfs:label "used input"@en ;
    rdfs:comment "Links a process run to an input artifact it used."@en ;
    rdfs:domain wfprov:ProcessRun ;
    rdfs:range wfprov:Artifact ;
    rdfs:subPropertyOf prov:used .

wfprov:wasOutputFrom a owl:ObjectProperty ;
    rdfs:label "was output from"@en ;
    rdfs:comment "Links an artifact to the process run that generated it."@en ;
    rdfs:domain wfprov:Artifact ;
    rdfs:range wfprov:ProcessRun ;
    rdfs:subPropertyOf prov:wasGeneratedBy .

wfprov:describedByWorkflow a owl:ObjectProperty ;
    rdfs:label "described by workflow"@en ;
    rdfs:comment "Links a workflow run to the workflow description it executes."@en ;
    rdfs:domain wfprov:WorkflowRun ;
    rdfs:range wfdesc:Workflow .

wfprov:describedByProcess a owl:ObjectProperty ;
    rdfs:label "described by process"@en ;
    rdfs:comment "Links a process run to the process description it executes."@en ;
    rdfs:domain wfprov:ProcessRun ;
    rdfs:range wfdesc:Process .

wfprov:wasEnactedBy a owl:ObjectProperty ;
    rdfs:label "was enacted by"@en ;
    rdfs:comment "Links a workflow run to the workflow engine that executed it."@en ;
    rdfs:domain wfprov:WorkflowRun ;
    rdfs:range wfprov:WorkflowEngine ;
    rdfs:subPropertyOf prov:wasAssociatedWith .

wfprov:describedByParameter a owl:ObjectProperty ;
    rdfs:label "described by parameter"@en ;
    rdfs:comment "This property associates a wfprov:Artifact to the wfdesc:Parameter description."@en ;
    rdfs:domain wfprov:Artifact ;
    rdfs:range wfdesc:Parameter .
